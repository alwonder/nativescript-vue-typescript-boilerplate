{
  "name": "nativescript-vue-ts-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate to make native applications with Typescript and NativeScript-Vue",
  "author": "Sergey Eliseev <thefury014@gmail.com>",
  "license": "MIT",
  "nativescript": {
    "id": "org.nativescript.application",
    "templateVersion": "v2",
    "tns-android": {
      "version": "6.5.0"
    },
    "tns-ios": {
      "version": "6.5.0"
    }
  },
  "scripts": {
    "serve:android": "tns debug android",
    "serve:ios": "tns debug ios --device \"iPhone 11\"",
    "devtools": "npx vue-devtools",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue ./app/"
  },
  "dependencies": {
    "@nativescript/theme": "^2.2.1",
    "@vue/devtools": "^5.3.3",
    "nativescript-socketio": "^3.3.1",
    "nativescript-toasty": "^3.0.0-alpha.2",
    "nativescript-vue": "^2.6.1",
    "nativescript-vue-devtools": "^1.4.0",
    "tns-core-modules": "^6.5.1",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.0.0",
    "vuex": "^3.3.0",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-loader": "^8.1.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-vue": "^6.0.0",
    "nativescript-dev-webpack": "^1.5.1",
    "nativescript-vue-template-compiler": "^2.6.0",
    "nativescript-worker-loader": "~0.11.0",
    "node-sass": "^4.13.1",
    "tns-platform-declarations": "^6.5.1",
    "typescript": "^3.9.5",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.1"
  }
}
